		"eslint-plugin-storybook": "^0.6.12",
		"husky": "^8.0.3",
		"postcss": "^8.4.24",
		"postcss-loader": "^7.3.3",
		"postcss-preset-env": "^8.5.1",
		"prettier": "^2.8.8",
		"rollup": "^3.25.3",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"storybook": "^7.0.23",
		"systemjs-webpack-interop": "^2.3.7",
		"tailwindcss": "^3.3.2",
		"webpack": "^5.88.0",
		"webpack-merge": "^5.9.0"
	},
