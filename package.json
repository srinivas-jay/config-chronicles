{
	"name": "@inscriptors/config-chronicles",
	"version": "0.0.1-alpha.1",
	"description": "A treasury of configuration files empowering organisational development journeys.",
	"keywords": [
		"eslint",
		"react",
		"typescript",
		"rollup",
		"postcss",
		"webpack",
		"storybook"
	],
	"homepage": "https://github.com/inscriptors/config-chronicles#readme",
	"bugs": {
		"url": "https://github.com/inscriptors/config-chronicles/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inscriptors/config-chronicles.git"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"author": "Srinivas Jay <srini.j135@gmail.com>",
	"main": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx .",
		"lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
		"prepare": "husky install",
		"prerelease": "standard-version --prerelease alpha && npm publish --verbose && git push --follow-tags origin main"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"npx prettier --write",
			"npx eslint --fix"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@babel/core": "^7.22.5",
		"@babel/plugin-transform-runtime": "^7.22.5",
